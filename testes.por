programa
{
	
	funcao inicio()
	{
		real num1,num2,res
		escreva("\nEntre com o num1:")
		leia(num1)
		escreva("\nEntre com o num2:")
		leia(num2)
		res= num1+num2
		escreva("\nSoma : ",res)
		res= num1-num2
		escreva("\nDiferença : ",res)
		res= num1*num2
		escreva("\nMultiplicação : ",res)
		res= num1/num2
		escreva("\nDivisão : ",res)
}
